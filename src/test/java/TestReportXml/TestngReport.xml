<?xml version="1.0" encoding="UTF-8"?>
<!-- parallel="tests" 多个<test></test>可同时并发执行，thread-count="10" 并发最大线程数 -->
<suite name="Selenium测试报告" configfailurepolicy ="continue" parallel="tests" thread-count="50">
    <test name="谷歌-搜索自动化测试系统流程" junit="false" verbose="3" parallel="false" thread-count="50" annotations="javadoc" time-out="60000" enabled="true" skipfailedinvocationcounts="true" preserve-order="true" allow-return-values="true">
        <parameter name="browser" value="chrome" /><!-- 指定浏览器启动 chrome/firefox-->
        <parameter name="profile" value="false" /><!-- 启动是否需要加载个人配置，true/false -->
        <classes>
            <class name="TestCases.SearchProcess"/>
                <methods>
                    <exclude name="Precondition" />
                    <include name="case1" />
                    <include name="case2" />
                    <!--<include name="case3"/>
                    <include name="case4" />
                    <include name="case5" />
                    <include name="case6" />
                    <include name="case7" />
                    <include name="case8" />
                    <include name="case9" />
                    <include name="case_10" />
                    <include name="case_11" />
                    <include name="case_12" />
                    <include name="case_13" />
                    <include name="case_14" />
                    <include name="case_15" />
                    <include name="case_16" /> -->
                    <exclude name="" />
                </methods>
        </classes>
    </test>
    <test name="火狐-搜索自动化测试系统流程" junit="false" verbose="3" parallel="false" thread-count="50" annotations="javadoc" time-out="60000" enabled="true" skipfailedinvocationcounts="true" preserve-order="true" allow-return-values="true">
        <parameter name="browser" value="chrome" />
        <parameter name="profile" value="false" />
        <classes>
            <class name="TestCases.SearchProcess"/>
                <methods>
                    <exclude name="Precondition" />
                    <include name="case1" />
                    <include name="case2" />
                    <exclude name="" />
                </methods>
        </classes>
    </test>
    ------------------------------------------------------------------------------------------------------
    <!-- 调用的监听 -->
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>
</suite>
